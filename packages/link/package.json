{
  "name": "link",
  "version": "0.0.0",
  "description": "The link application for the Boreal Director orchestation suite",
  "main": "src/link.js",
  "bin": "dist/link.js",
  "repository": "https://github.com/borealsystems/Director",
  "author": "Oliver Herrmann",
  "license": "MIT",
  "scripts": {
    "release": "rm -rf build && rm -rf dist && yarn workspace link run build && yarn workspace link run package",
    "package": "pkg build/link.js --out-path dist",
    "build": "babel src/ -d build/",
    "dev": "NODE_ENV=development nodemon --signal SIGHUP --exec babel-node src/link.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^2.0.2"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "chalk": "^3.0.0",
    "debug": "^4.1.1",
    "elgato-stream-deck": "^3.3.1",
    "fetch": "^1.1.0",
    "graphql": "^14.6.0",
    "graphql-request": "1.8.2",
    "isomorphic-unfetch": "^3.0.0",
    "jimp": "^0.14.0",
    "lodash": "^4.17.15",
    "mdns": "^2.5.1",
    "nicely-format": "^1.1.0",
    "open": "^7.1.0",
    "sharp": "^0.25.4",
    "shortid": "^2.2.15",
    "smart-promisify": "^1.0.5",
    "systray": "^1.0.5",
    "urql": "^1.9.8"
  }
}
